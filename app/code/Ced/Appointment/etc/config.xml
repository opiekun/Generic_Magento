<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <booking>
            <appointment_config>
                <enable>1</enable>
                <appointment_link_enabled>10</appointment_link_enabled>
                <appointment_link_title>Find Appointments</appointment_link_title>
                <banner_width>800</banner_width>
                <banner_height>200</banner_height>
            </appointment_config>
            <booking_products>
                <type>
                    <appointment>*</appointment>
                </type>
            </booking_products>
        </booking>
    </default>
</config>